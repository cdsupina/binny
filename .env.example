# Binny inventory directory (required)
BINNY_INVENTORY_DIR=/path/to/inventory/files

# Part namer configuration files
BINNY_MATERIALS_FILE=/path/to/.config/binny/part_namer/materials.md
BINNY_PREFIXES_FILE=/path/to/.config/binny/part_namer/prefixes.md

# McMaster-Carr subscribed parts file path
MMC_SUBSCRIBED_PARTS_FILE=/path/to/subscribed_parts.txt

# McMaster-Carr credentials
MCMASTER_USERNAME=your-email@example.com
MCMASTER_PASSWORD=your-password
MCMASTER_CERT_PATH=~/.config/mmc/certificate.pfx
MCMASTER_CERT_PASSWORD=your-cert-password
